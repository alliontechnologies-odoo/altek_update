<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record model="ir.ui.view" id="product_template_indent_form_view">
        <field name="name">product.template.indent</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//page[@name='purchase']" position="after">
                <page name="indent" string="Indent">
                    <group>
                        <group>
                            <field name="indent_commission"/>
                        </group>
                    </group>
                </page>
            </xpath>
        </field>
    </record>
</odoo>
